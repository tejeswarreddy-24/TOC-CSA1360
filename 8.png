#include <stdio.h>
#include <string.h>
char input[100];
int start(char c) 
{
	if (c == 'b')
	 {
		return 1;
	}
	return 0;
}
int end(char c)
 {
	if (c == 'a')
	 {
		return 1;
	}
	return 0;
}
int main()
 {
	printf("Enter a string: ");
	scanf("%s", input);
	if (start(input[0]) && end(input[strlen(input) - 1])) 
	{
		printf("Accepted\n");
	} else {
		printf("Rejected\n");
	}
	return 0;
}
